// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Rental{
  id String @id @default(uuid())
  rooms Int
  location String
  rentalPrice Decimal
  purchasePrice Decimal?
  type String
}